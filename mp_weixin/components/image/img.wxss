/* components/image/img.wxss */


.img-box{
  display: block;
  position: relative;
  overflow: hidden;
  padding-bottom: inherit;
}
.is-contain.img-box > img {
  object-fit: contain;
  font-family: "object-fit: contain";
}

.js-cover{
  object-fit: cover;
  width: 100%;
  height: 100%;
}

.is-cover.img-box > img {
  object-fit: cover;
  font-family: "object-fit: cover";
}
.img-box .img {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.img-box .img{
  transition: transform .3s ease-in-out;
}

.img-box .img{
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}



.img-box:after {
  content: ' ';
  display: block;
  height: 0;
  width: 100%;
  padding-bottom: 75%;
}
.img-box.img-scale:hover img,.img-box.img-scale:hover .img{
  transform: scale(1.2);
}
